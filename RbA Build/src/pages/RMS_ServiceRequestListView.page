<apex:page controller="RMS_serviceRequestListViewMgr">
    <style>
        img.pageTitleIcon{
        display:none;
        }
        
        .listViewportWrapper .listViewport .topNav h2.title {
        font-size: 1.1em;
        }
        
        #j_id0:j_id2:out1:ALL:j_id11:OrderList_wrapper {
        display: none;
        }
        
    </style>
    
    
    <br/>
    <br/>
    <apex:pageBlock >
        
        <apex:pageBlockSection >
            <apex:form >
                <apex:outputLabel style="font-weight: bold; margin-right: 10px;">Select View: </apex:outputLabel>
                <apex:selectList id="selectView" required="true" title="PickList1" size="1" value="{!propPickValSelected}" style="width: 150px;" label="Select View:">
                    <apex:selectOptions value="{!PickLstValue}" />
                    <apex:actionSupport event="onchange" rerender="out1" />
                </apex:selectList> 
            </apex:form>
        </apex:pageBlockSection>
        
        <br/>
        <apex:pageBlockSection id="out1" columns="1">
            <c:RMS_requestListViewComponent listViewName="{!propPickValSelected}" id="dynamic" rendered="{!IF(propPickValSelected!=' ',true,false)}"/>
            <c:RMS_requestListViewComponent listViewName="Service Requests - All" id="ALL"/>
            <!--<c:RMS_requestListViewComponent listViewName="Service Reqeusts - Product Ordered" id="PO"/>-->
        </apex:pageBlockSection>
    </apex:pageBlock>
</apex:page>