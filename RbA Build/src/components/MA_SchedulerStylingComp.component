/*     
=========================================================
Forms
=========================================================
*/


.ma-input {
    -webkit-appearance: none;
    box-shadow: none !important;
    -webkit-box-shadow: none !important;
    font-size: 13px;
    background-color: #fff;
    color: #54698d;
    font-family: Arial,Helvetica,sans-serif;
    line-height: 17px;
    border: 1px solid #d8dde6;
    border-radius: 4px;
    width: auto;
    -webkit-transition: border 0.1s linear, background-color 0.1s linear;
    transition: border 0.1s linear, background-color 0.1s linear;
    display: inline-block;
    font-family: 'Salesforce Sans', Arial, Helvetica, sans-serif;
}

textarea.ma-input {
    padding: 14px;
}

.ma-input:focus {
	border-color: #1589EE;
	outline:0;
	-webkit-outline: 0;
}

.ma-input-label {
	    display: inline-block;
	    color: #54698d;
	    font-size: 13px;
	    line-height: 13px;
	    margin:3px 0;
}

.ma-section-title {
    font-size: 14px;
    line-height: 17.5px;
    text-transform: uppercase;
    letter-spacing: 1px;
    color: #54698d;
    margin: 0;
    padding: 8px 16px;
    border-top: 1px solid rgba(0,0,0,0.05);
    background: #f4f6f9;
    border-bottom: 1px solid rgba(0,0,0,0.05);
}

.ma-section-title.ma-section-title-large {
    padding: 16px;
    font-size: 24px;
    line-height: 30px;
    margin: 0;
    font-weight: 300;
    border-top: 1px solid rgba(0,0,0,0.1);
    border-bottom: 1px solid rgba(0,0,0,0.1);
}

.ma-icon {
	margin:0;
	padding:0;
	max-width:16px;
}

.ma-form-control-wrap {
    margin-top: 0px;
    float: left;
    margin: 0px 30px;
}

.ma-form-control {
	display: inline-block;
}

.ma-form-control.icon-left .ma-input {
    padding-left: 46px;
}

.ma-form-control.icon-left .ma-icon.icon-left {
    position: absolute;
    top: 13px;
